  "version": "0.2.0",
  "configurations": [
    {
      "name": "SimpleTester Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/SimpleTester/bin/Debug/netcoreapp2.1/SimpleTester.dll",
      "args": [],
      "cwd": "${workspaceFolder}/SimpleTester",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "OdbDump Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "buildODbDump",
      "program": "${workspaceFolder}/ODbDump/bin/Debug/netcoreapp2.1/ODbDump.dll",
      "args": [".", "compact"],
      "cwd": "C:/Research/bug/big",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
